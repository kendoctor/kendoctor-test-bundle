<div class="paper_itemgroup_form">
    <h4>{{ entity.typeDesc }}</h4>  
    <form class="paper_itemgroup_edit_form" action="{{ path('itemgroup_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
            <input type="hidden" name="_method" value="PUT" />
            {{ form_widget(edit_form) }}
            <p>
                <button type="submit" onclick="return paper_itemgroup_update(this)">Edit</button>
                <button type="button" onclick="return paper_itemgroup_edit_cancel(this)">取消</button>
            </p>
        </form>
</div>